<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gsau.provider_dept_8082.dao.DepartmentDao">
    <!--插入-->
    <insert id="addDept" >
        INSERT INTO Department(dnName,dbSource) VALUE (#{dnName},#{dbSource});
    </insert>
    <select id="findById" resultType="com.gsau.api_8081.entities.Department" >
        select  * from Department where id=#{id}
    </select>
    <select id="findAll" resultType="com.gsau.api_8081.entities.Department" >
        SELECT * FROM  Department;
    </select>
</mapper>